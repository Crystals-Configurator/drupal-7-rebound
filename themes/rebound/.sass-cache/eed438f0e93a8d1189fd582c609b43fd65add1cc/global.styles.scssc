	font-size:2.286em;
	line-height:1.2;
}

h2 {}

h3 {}

h4 {}

h5 {}

h6 {}

p {
	margin-bottom:1em;
}

b,
strong {}

em {}

dfn {}

sup {}

sub {}

del {}

ins {}

blockquote {}

cite {}

q {}

address {}

ul {}

ol {}

li {}

dl {}

dd {}

