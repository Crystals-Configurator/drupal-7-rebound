}

.forum-add-node .af-button-large:before,
.topic-reply-allowed .af-button-large:before { @include icon-edit; }
.last-post-link .af-button-large:before { @include icon-down-open; }

.forum-tools {
	margin:0;
}

#forum-statistics {
	background: $dark_light;
	color: $page;
	padding: 10px;
	margin-bottom:2em;
}

#forum-statistics-header {
	font-size: 1.714em;
	font-family: "OpenSansLight";
	margin-bottom: 0.3em;
	padding:0;
}

.forum-statistics-sub-header {
	padding:0;
	font-size:0.857em;
	color:$medium;
}

.forum-statistics-sub-body {
	padding:0;
	margin-bottom:10px;
}


/* =============================================================================
 *   Taxonomy page Styles
 * ========================================================================== */
 
.page-taxonomy #page-title:before {
	@include icon-tag;
	font-size:0.7em;
	margin-right:0;
}

/* =============================================================================
 *   Shoutbox Styles
 * ========================================================================== */
 
#block-shoutbox-shoutbox {
	position:relative;
	z-index:30;
	margin-top: -40px;
	width:65%;
	
	table {
		margin:0;
		
		tbody {
			border-top:none;
		}
	}
	
	.block-title {
		color:$page;
	}
	
	.shoutbox-interval-msg {
		float:none;
		color:$light;
	}
	
	.form-type-textfield input {
		background:$page;
	}
	
	fieldset {
		border:none;
	}
	
	#shoutbox-body {
		width:auto;
	}
	
	#shoutbox-add-form {
		width:auto;
	}
	
	.shoutbox-add-form input#edit-message {
		width:75%;
	}
}


/* =============================================================================
 *   Wowhead Tooltip
 * ========================================================================== */
.wowhead-tooltip {
  tbody tr,
  tbody tr.even,
  tbody tr.odd {
    border:none;
    background:none;
  }
}


/* =============================================================================
 *   Messeges
 * ========================================================================== */
.privatemsg-message-participants {
  border-bottom:1px solid $medium_light;
}

.privatemsg-author-avatar {
    float: left;
    width: 80px;
    height: 80px;
    position: relative;
    margin-top: 5px;
    margin-right: -5px;
    line-height: 0;
    z-index:10;
    padding: 0;

    &:after {
      content: "";
      width: 0;
      height: 0;
      position: absolute;
      top: 35px;
      right: 0;
      border-left: 5px solid transparent;
      border-bottom: 5px solid $light;
      border-top: 5px solid $light;
    }
    
    &:before {
      content: "";
      width: 5px;
      height: 30px;
      position: absolute;
      background: $light;
      top: 5px;
      right: 0;
      z-index:10;
      box-shadow: 0 40px 0 $light;
    }
    
    img {
      clip: rect(5px,80px,75px,5px);
      position: absolute;
      left: 0;
      top: 0
    }
  }


