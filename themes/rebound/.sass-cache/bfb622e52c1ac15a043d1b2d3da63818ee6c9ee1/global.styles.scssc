 * ========================================================================== */

#tasks {
  margin-top: -30px;
	float: right;
	position: relative;
	width: 100%;
	
	.tasks-toggle {
		width: 21px;
		height: 21px;
		background: $primary_color;
		float: right;
		
		span:before {
